{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
          "rewrites": [
            {
              "source": "/api/search2",
              "run": {
                "serviceId": "search",
                "region": "us-central1"
              }
            },
            {
              "source": "/api/**",
              "run": {
                "serviceId": "search",
                "region": "us-central1"
              }
            },
            {
              "source": "**",
              "destination": "/index.html"
            }
          ]
  }
}
